<div class="page-header">
  <div class="breadcrumb"><i class="icon-location"></i><span><%= location %></span></div>
  <div class="archive-nav">
    <div class="home-entry">盒子</div>
    <ul class="list-entry">
      <% theme.page_nav.forEach(function(item, i){ %>
        <% for(var j in item) { %>
          <li class="item-entry">
            <a href="<%= item[j] %>"><%= j %></a>
          </li>
        <%} %>
      <% }) %>
    </ul>
  </div>
  <div class="box-blog-info">
    <a class="avatar" href="/">
      <img src="/images/avatar.jpg" alt="" />
    </a>
    <div class="info">
      <h3 class="name">
        <%= theme.blog_name %>
      </h3>
      <div class="slogan">
        <%= theme.slogan %>
      </div>
    </div>
  </div>
</div>
